set number
set nocompatible
set autoindent smartindent shiftround
set shiftwidth=4 
set tabstop=4
set softtabstop=4
set title
set ruler
set cursorcolumn
set cursorline
filetype plugin on

syntax on
set background=dark
colorscheme hybrid

""inoremap ' ''<Esc>i
""inoremap " ""<Esc>i
""inoremap ( ()<Esc>i
inoremap { {<CR>}<ESC>O
""inoremap [ []<Esc>i
""inoremap < <><Esc>i

"set listchars=tab:\|\ 
"set list

set vb t_vb=

set clipboard=unnamed
